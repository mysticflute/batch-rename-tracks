{
  "name": "batch-rename-tracks",
  "version": "0.0.1",
  "description": "Rename music track files",
  "author": "Nathan McWilliams",
  "license": "MIT",
  "main": "index.js",
  "bin": {
    "batch-rename": "index.js"
  },
  "scripts": {
    "help": "node ./index.js --help",
    "example-single": "node ./index.js yaml --dry-run --disk 1 examples/single-disk/config.yaml examples/single-disk/soundtrack",
    "example-multi": "node ./index.js yaml --dry-run --auto-disk examples/multi-disk/config.yaml examples/multi-disk/soundtrack/disk2"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "jest": "^29.7.0"
  }
}
